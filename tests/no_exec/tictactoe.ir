#F_BEGIN;
    #F_NAME fill_array;
    #F_ARGS_BEGIN;
        @Array len Int arr;
        @Int c;
    #F_ARGS_END;
    #F_VARS_BEGIN;
        @Array #1 Int range;
        @Int i;
        @Int len;
    #F_VARS_END;
    #F_BODY_BEGIN;
        #len len arr;
        #range range #0 len #1;

        #FOR i IN range;
            #[] arr i c;
        #ENDFOR;
    #F_BODY_END;
#F_END;

